{
  "name": "Steam Level Grabber",
  "description": "Extract level from steam profile",
  "version": "1.5",
  "manifest_version": 3,
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "scripting",
    "debugger"
  ],
  "host_permissions": [
    "*://*.steampowered.com/*",
    "*://steamcommunity.com/*",
    "https://steamcommunity.com/dev/apikey"
  ],
  "background": {}
}